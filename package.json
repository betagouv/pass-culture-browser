  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "prebuild": "yarn fontello",
    "build": "node scripts/build.js && ./scripts/build_redirects.sh",
    "fontello": "./scripts/fontello.sh",
    "licenses": "node_modules/.bin/license-reporter save --json licenses.json",
    "ngrok": "ngrok start -config=./ngrok.yml --all",
    "pgbuild": "yarn build && node scripts/pgbuild.js",
    "pgbuild-prod": "yarn build && PG_ENV=production node scripts/pgbuild.js",
    "prettier": "$(yarn bin)/prettier --write 'src/**/*.js'",
    "start": "node scripts/start.js",
    "test:cafe": "testcafe --assertion-timeout 8000 --selector-timeout 20000 -S -s testcafe_screenshots 'chrome:headless' testcafe",
    "test:unit": "jest --env=jsdom",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "test:visual": "./scripts/tests/visual.sh",
    "symlink-shared": "if [ -d \"../shared\" ]; then cd node_modules && rm -rf pass-culture-shared && ln -sf ../../shared pass-culture-shared && cd ../../shared && yarn run compile; fi"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/hooks/precommit.sh"
    }
  }
