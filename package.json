  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "lint-staged": "^7.0.4",
    "prettier": "1.12.1",
    "testcafe": "^0.20.4"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "ngrok": "ngrok start -config=./ngrok.yml --all",
    "pgbuild": "yarn build && node scripts/pgbuild.js",
    "pgbuild-prod": "yarn build && PG_ENV=production node scripts/pgbuild.js",
    "postinstall": "cd node_modules && ln -sf ../../shared pass-culture-shared && cd ../../shared && yarn run compile",
    "prettier": "$(yarn bin)/prettier --write 'src/**/*.js'",
    "start": "node scripts/start.js",
    "test": "node scripts/jesttest.js --env=jsdom",
    "testcafe": "testcafe 'chrome:headless' testcafe"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/testcafe/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ],
    "presets": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier"
    }
  },
  "homepage": "."
}
